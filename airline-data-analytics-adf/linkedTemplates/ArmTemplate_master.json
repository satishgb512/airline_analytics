{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "airline-data-analytics-adf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureKeyVault_LS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://airlinekv.vault.azure.net/"
		},
		"integrationRuntime1_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/61b3c659-6415-431d-b597-1cbdec97ece8/resourcegroups/satish-rg/providers/Microsoft.DataFactory/factories/satishadf20240615/integrationruntimes/integrationRuntime1"
		},
		"PostgreSql_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"PostgreSql_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "airline_db"
		},
		"PostgreSql_LS_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "satish"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "airline-data-analytics-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureKeyVault_LS_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_LS_properties_typeProperties_baseUrl')]"
					},
					"integrationRuntime1_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('integrationRuntime1_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"PostgreSql_LS_properties_typeProperties_server": {
						"value": "[parameters('PostgreSql_LS_properties_typeProperties_server')]"
					},
					"PostgreSql_LS_properties_typeProperties_database": {
						"value": "[parameters('PostgreSql_LS_properties_typeProperties_database')]"
					},
					"PostgreSql_LS_properties_typeProperties_username": {
						"value": "[parameters('PostgreSql_LS_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}