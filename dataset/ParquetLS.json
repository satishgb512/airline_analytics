{
	"name": "ParquetLS",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"schemaname": {
				"type": "string"
			},
			"table": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().table",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().schemaname,'/',dataset().table)",
					"type": "Expression"
				},
				"container": "airlineraw"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}